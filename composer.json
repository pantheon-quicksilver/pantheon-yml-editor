{
  "name": "pantheon-quicksilver/pantheon-yml-editor",
  "description": "A utility package for editing the pantheon.yml file.",
  "type": "composer-plugin",
  "require": {
    "composer-plugin-api": "^1.0 || ^2.0",
    "symfony/yaml": "^3 || ^4 || ^5",
    "consolidation/comments": "^1.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.6"
  },
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "PantheonYmlEditor\\": "src/"
    }
  },
  "extra": {
    "class": "PantheonYmlEditor\\Plugin"
  },
  "scripts": {
    "cs": "phpcs --standard=PSR2 src",
    "fix": "phpcbf --standard=PSR2 src"
  },
  "authors": [
    {
      "name": "Kyle Taylor",
      "email": "kyle.taylor@pantheon.io"
    }
  ]
}
